{
  "name": "gql-integratio",
  "version": "0.1.0",
  "bin": {
    "gql-integratio": "bin/gql-integratio.ts"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "destroy": "cdk destroy --all",
    "deploy": "cdk deploy --all --require-approval never",
    "deploy:dev": "cdk deploy --all --no-rollback",
    "docgen": "graphdoc -s app/schema.graphql -o .gqldocs -f"
  },
  "devDependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "@types/aws-lambda": "^8.10.92",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.3.0",
    "aws-cdk": "2.13.0",
    "aws-lambda": "^1.0.7",
    "dotenv": "^16.0.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "~4.5.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.13.0",
    "constructs": "^10.0.62",
    "graphql": "^16.3.0",
    "source-map-support": "^0.5.21"
  }
}
